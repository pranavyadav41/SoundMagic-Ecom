   <!-- MY ORDERS SECTION BEGIN -->
   <% orders.forEach((order)=>{ %>
   <div class="order-container">
    <!-- Repeat the following block for each order -->
    <% order.products.forEach((product)=>{ %>
    <div class="order-item">
        <img class="product-image" src="/productImages/<%= product.productId.image[0] %>" alt="Product Image" style="height: 80px;width: 80px;">
        <div class="product-details">
        <div class="d-flex">
        <div>
            <div class="product-name"><b><%= product.productId.productName %></b></div>
            <div class="product-price">&#8377;<%= product.productId.offerPrice %></div>
        </div>
        <div class="mt-3 ml-5">
            
        </div>
    </div>
            
        </div>
        <div class="d-flex">
        <div class="order-status mr-5 mt-2"><span style="color: green;"><span class="dot"></span><b><%= order.OrderStatus %></b></span></div>
        <a href="/orderDetail/<%= product.productId._id %>"   class="btn btn-dark">View details</a>
    </div>
    </div>
    <!-- End of order item -->
    <% }); %>

    <!-- Repeat the above block for each order -->
</div>
<% }); %>
<!-- MY ORDERS SECTION END -->
  
