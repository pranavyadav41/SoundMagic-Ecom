    <style>
        body {
            font-family: 'Arial', sans-serif;
            background-color: #f4f4f4;
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }
        .dot {
  height: 8px;
  width: 8px;
  background-color: #266233;
  border-radius: 50%;
  display: inline-block;
  margin-right: 5px;
}

        .order-container {
            max-width: 800px;
            margin: 20px auto;
            background-color: #fff;
            padding: 20px;
            box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
        }

        .order-item {
            border-bottom: 1px solid #ddd;
            padding: 10px 0;
            display: flex;
            align-items: center;
        }

        .product-image {
            max-width: 100px;
            margin-right: 20px;
        }

        .product-details {
            flex-grow: 1;
            text-align: left;
        }

        .product-name {
            font-size: 18px;
            font-family:Arial, Helvetica, sans-serif;
            margin-bottom: 5px;
        }

        .product-price {
            font-size: 16px;
            color: #888;
            margin-bottom: 5px;
        }

        .order-subtotal {
            font-weight: bold;
            font-size: 18px;
            margin-bottom: 5px;
        }

        .view-details-btn {
            padding: 10px;
            cursor: pointer;
            background-color: #3498db;
            color: #fff;
            border: none;
            font-weight: bold;
            transition: background-color 0.3s ease;
        }

        .view-details-btn:hover {
            background-color: #2980b9;
        }
    </style>
    
</head>

